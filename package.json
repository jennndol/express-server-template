    "jsonwebtoken": "^8.2.2",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.19.1",
    "rimraf": "^2.6.2"
  }
